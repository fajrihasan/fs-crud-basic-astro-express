generator client {
  provider = "prisma-client-js"
}

// Connection URL property moved as per Prisma 5+/7+ requirements.
// The datasource should no longer use `url` directlyâ€”handled via prisma.config.ts now.
datasource db {
  provider = "postgresql"
}

model barang {
  id          Int    @id @default(autoincrement())
  name        String
  price       Int
  description String
  image       String
}
